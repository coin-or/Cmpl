#!/bin/bash

export PYTHONPATH=$PYTHONPATH:"$CmplProgPath"pyCmpl/lib
export PYTHONSTARTUP="$CmplProgPath"pyCmpl/lib/pyCmplShell.py
export CMPLBINARY="$CmplProgPath"bin/cmpl


