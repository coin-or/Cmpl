# Standardmodell der linearen Optimierung
# Alternative Schreibweisen

const:
	c[1..2] = ( 1, 2 );
	c[3]    = 3;
	b[..2]  = ( 15, 20 );
	A[,1]   = ( 5.6, 9.8 );
	A[,2..] = (( 7.7, 10.5 ),
			   ( 4.2, 11.1 ));

var:
	x[1]: integer; x[2..3]: real;

objective:
	ziel: sum{i=1(1)3: c[i] * x[i]};

constraint:
	{i=1(1)2: A[i,]T * x[] <= b[i]; }
	{i=1(1)3: x[i] >= 0; }




